        "ts-loader": "^9.4.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths-webpack-plugin": "^4.1.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4"
