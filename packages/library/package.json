        "@babylonjs/inspector": "7.12.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.3",
        "@babel/preset-env": "^7.24.3",
        "@babel/preset-typescript": "^7.24.1",
        "@babel/register": "^7.23.7",
        "@dvmstudios/reactylon-common": "*",
        "@types/acorn": "^6.0.0",
